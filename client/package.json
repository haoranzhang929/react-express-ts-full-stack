{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.22",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "start": "npm run lint:client && npm test && react-scripts start",
    "build": "npm run lint:client && npm test && react-scripts build",
    "test": "cross-env CI=true react-scripts test",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:client": "echo 'Client Lint Checking...' && eslint --ext .js,.jsx,.ts,.tsx src && echo 'Lint Checking Completed'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000"
}
